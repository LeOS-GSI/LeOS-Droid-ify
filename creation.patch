+fun CharSequence.unhex(): ByteArray? {
+	return if (length % 2 == 0) {
+		val ints = windowed(2, 2, false).map {
+			val high = it[0].halfByte()
+			val low = it[1].halfByte()
+			if (high >= 0 && low >= 0) {
+				(high shl 4) or low
+			} else {
+				-1
+			}
+		}
+		if (ints.any { it < 0 }) null else ints.map { it.toByte() }.toByteArray()
+	} else {
+		null
 	}
